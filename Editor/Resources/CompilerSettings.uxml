                <ui:Button display-tooltip-when-elided="true" name="CompilerPathDialog" style="flex-wrap: nowrap; flex-shrink: 1; flex-basis: 3%; margin-left: 0; padding-left: 7px; background-image: resource(&apos;file_icon&apos;); -unity-background-scale-mode: scale-to-fit; -unity-slice-left: 3; -unity-slice-top: 3; -unity-slice-right: 3; -unity-slice-bottom: 3; -unity-background-image-tint-color: rgb(255, 255, 255); font-size: 13px; -unity-font-style: bold; padding-top: -18px;" />
                <ui:TextField picking-mode="Ignore" name="CompilerPathField" style="flex-basis: 67%; margin-left: 0; margin-right: 1px; margin-top: 1px; margin-bottom: 1px; flex-shrink: 1;" />
            </ui:VisualElement>
            <ui:VisualElement name="GPU" style="flex-direction: row;">
