<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" editor-extension-mode="False">
    <ui:VisualElement name="CompilerSettings" style="border-left-width: 1px; border-right-width: 1px; border-top-width: 1px; border-bottom-width: 1px; border-left-color: rgb(68, 68, 68); border-right-color: rgb(68, 68, 68); border-top-color: rgb(68, 68, 68); border-bottom-color: rgb(68, 68, 68);">
        <ui:Foldout text="Compiler Settings" name="CompilerFoldout">
            <ui:VisualElement name="Compiler" style="flex-direction: row;">
                <ui:Label text="Compiler" display-tooltip-when-elided="true" name="CompilerLabel" style="flex-basis: 30%; flex-direction: row; padding-left: 0; padding-right: 0; padding-top: 0; padding-bottom: 0; min-width: auto; width: auto; flex-grow: 0; flex-shrink: 0;" />
                <ui:DropdownField index="-1" choices="System.Collections.Generic.List`1[System.String]" name="CompilerDrop" style="flex-basis: 70%; margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 0;" />
            </ui:VisualElement>
            <ui:VisualElement name="CompilerPath" style="flex-direction: row; flex-shrink: 0;">
